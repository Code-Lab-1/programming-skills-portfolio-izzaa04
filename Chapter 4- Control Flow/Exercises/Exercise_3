#running if block
alien_color="green"
if alien_color=="green":
  print("Yay! You earned 5 points!")
elif alien_color=="yellow":
  print("Yay! You earned 10 points!")
else:
  print("Yay! You earned 15 points!")
#running elif block
  alien_color="yellow"
if alien_color=="green":
  print("Yay! You earned 5 points!")
elif alien_color=="yellow":
  print("Yay! You earned 10 points!")
else:
  print("Yay! You earned 15 points!")
#running else block
alien_color="red"
if alien_color=="green":
  print("Yay! You earned 5 points!")
elif alien_color=="yellow":
  print("Yay! You earned 10 points!")
else:
  print("Yay! You earned 15 points!")